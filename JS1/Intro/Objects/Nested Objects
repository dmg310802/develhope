//Nested Object

let nested = {
"subObject" : {
    "value": 1,
    "value2": 2,
    "valuen": {
        "subvalue": 3
        }
    }
};


//to check if a value exists within an object you add a question mark at the end ?

//Example
console.log(nested.subObject.dNE?.value)

//To find errors you can
if(nested.subObject?.dNE?.value === undefined){
console.log("key dNE does not exist");
}


//To use a default value 
let value = nested.subObject.dNE?.value  === undefined ? 0 : nested.subObject.dNE.value;
if(nested.subObject?.dNE?.value === undefined){
console.log("key dNE does not exist");
}


console.log(nested.subObject.value);